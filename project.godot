; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Nat Music Programme"
config/description="Nat Music Programme"
config/version="1.0.1"
run/main_scene="res://Scenes/MainScreen.tscn"
config/features=PackedStringArray("4.4", "Mobile")
run/max_fps=30
run/low_processor_mode=true
boot_splash/use_filter=false
boot_splash/image="res://Assets/NMP_Icon.png"
config/icon="res://Assets/NMP_Icon.ico"
config/windows_native_icon="res://Assets/NMP_Icon.ico"

[audio]

buses/default_bus_layout="res://Assets/AudioBusLayout.tres"

[autoload]

MasterDirectoryManager="*res://Scripts/Globals/MasterDirectoryManager.gd"
GeneralManager="*res://Scripts/Globals/GeneralManager.gd"

[debug]

gdscript/warnings/unreachable_code=2
gdscript/warnings/untyped_declaration=2
gdscript/warnings/inferred_declaration=2

[display]

window/size/viewport_width=960
window/size/viewport_height=540
window/size/mode=3
window/stretch/mode="canvas_items"

[editor]

naming/scene_name_casing=1
naming/script_name_casing=1

[editor_plugins]

enabled=PackedStringArray("res://addons/ExportDataSetter/plugin.cfg", "res://addons/NatCustomExporter/plugin.cfg")

[global_group]

HatBar=""
Header=""
Background=""
Separator=""
ScrollContainer=""
CLIOutputLabel=""
ProfileLabel=""

[gui]

theme/custom_font="res://Assets/Fonts/Main.ttf"

